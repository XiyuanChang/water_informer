{
    "name": "informer",
    "model_type": "informer",
    "dataset": "ClimateDatasetV2C",
    "root_dir": "./climate_washed", 
    "threshold": [1, 1, 1],
    "normalize": "LogMinMax",
    "y_normalize": true,
    "n_gpu": 1,
    "test_station": 482,
    "valid_threshold": 2,
    "metric_threshold": 0,
    "x_feature": ["runoff", "pr", "sph", "srad", "tmmn", "tmmx", "pet", "etr", "ph", "Conduc", "Ca", "Mg", "K", "Na", "NH4", "NO3", "Cl", "SO4", "distNTN", "LAI", "FAPAR", "NPP", "datenum", "sinT", "cosT"],
    "y_feature": ["00010","00095","00300","00400","00405","00600","00605","00618","00660","00665",
              "00681","00915","00925","00930","00935","00940","00945","00955","71846","80154",
            "runoff", "pr", "sph", "srad", "tmmn", "tmmx", "pet", "etr", "ph", "Conduc", "Ca", "Mg", "K", "Na", "NH4", "NO3", "Cl", "SO4", "distNTN", "LAI", "FAPAR", "NPP", "datenum", "sinT", "cosT",
            "HYDRO_DISTURB_INDX", "BAS_COMPACTNESS", "DRAIN_SQKM", "SNOW_PCT_PRECIP", "GEOL_REEDBUSH_DOM", "GEOL_REEDBUSH_DOM_PCT",
            "STREAMS_KM_SQ_KM",
            "STRAHLER_MAX",
            "MAINSTEM_SINUOUSITY",
            "BFI_AVE",
            "CONTACT",
            "PCT_1ST_ORDER",
            "PCT_2ND_ORDER",
            "PCT_3RD_ORDER",
            "PCT_4TH_ORDER",
            "PCT_5TH_ORDER",
            "PCT_6TH_ORDER_OR_MORE",
            "DDENS_2009",
            "STOR_NOR_2009",
            "NPDES_MAJ_DENS",
            "DEVNLCD06",
            "FORESTNLCD06",
            "PLANTNLCD06",
            "WATERNLCD06",
            "WOODYWETNLCD06",
            "EMERGWETNLCD06",
            "NITR_APP_KG_SQKM",
            "PHOS_APP_KG_SQKM",
            "PESTAPP_KG_SQKM",
            "ECO2_BAS_DOM",
            "ECO3_BAS_DOM",
            "NUTR_BAS_DOM",
            "HLR_BAS_DOM_100M",
            "PNV_BAS_DOM",
            "AWCAVE",
            "PERMAVE",
            "BDAVE",
            "OMAVE",
            "WTDEPAVE",
            "ROCKDEPAVE",
            "CLAYAVE",
            "SILTAVE",
            "KFACT_UP",
            "RFACT",
            "ELEV_MEAN_M_BASIN",
            "SLOPE_PCT",
            "ASPECT_DEGREES",
            "LAT_GAGE",
            "LNG_GAGE"
        ],
    "minmax_feature": ["LAI", "FAPAR", "NPP", "LAT_GAGE", "LNG_GAGE", "datenum", "sinT", "cosT", "00010", "00300", "00400"],
    "exclude": 0,
    
    "arch": {
        "in_dim": 74,
        "out_dim": 94,
        "dropout": 0.1
    },

    "seq_len": 365,
    "label_len": 96,
    "pred_len": 2,
    "d_model": 512,
    "n_heads": 4,
    "e_layers": 3,
    "d_layers": 2,
    "d_ff": 2048,
    "factor": 5,
    "attn": "full",
    "embed": "timeF",
    "freq": "h",
    "activation": "gelu",
    "distil": true,
    "mix": true,
    "output_attention": false,

    "data_loader": {
        "type": "",
        "args":{
            "batch_size": 512,
            "num_workers": 10
        }
    },
    
    "loss": "mse_loss",
    "metrics": [
        "kge", "r_squared", "pbias"
    ],
    "optimizer": {
        "type": "AdamW",
        "args":{
            "lr": 0.0001
        }
    },
    "lr_scheduler": {
        "type": "CosineAnnealingLR",
        "args": {
            "T_max": 300,
            "eta_min": 1e-6
        }
    },

    "trainer": {
        "epochs": 1,
        "save_dir": "unified_ablation_informer_pretrain_pred_2/baseline_dropout_0.1_665",
        "save_period": 2,
        "verbosity": 2,
        "log_epoch": 1,
        
        "early_stop": 10,

        "tensorboard": false
    }
} 